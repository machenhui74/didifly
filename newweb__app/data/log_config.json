{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "winston": {
      "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
    },
    "detailed": {
      "format": "%(asctime)s - %(name)s - %(levelname)s - %(module)s:%(funcName)s:%(lineno)d - %(message)s"
    }
  },
  "handlers": {
    "file": {
      "class": "logging.handlers.RotatingFileHandler",
      "filename": "./logs/app.log",
      "maxBytes": 10485760,
      "backupCount": 5,
      "encoding": "utf-8",
      "formatter": "winston"
    },
    "console": {
      "class": "logging.StreamHandler",
      "formatter": "winston",
      "stream": "ext://sys.stdout"
    }
  },
  "loggers": {
    "newweb__app": {
      "level": "INFO",
      "handlers": [
        "file",
        "console"
      ],
      "propagate": false
    },
    "werkzeug": {
      "level": "INFO",
      "handlers": [
        "file"
      ],
      "propagate": false
    }
  },
  "root": {
    "level": "INFO",
    "handlers": [
      "file",
      "console"
    ]
  }
}